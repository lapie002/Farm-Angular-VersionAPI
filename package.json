{
  "name": "rxp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "local": "ng serve -dev --env=local",
    "lint": "tslint \"src/**/*.ts\"",
    "tslint-autofix": "tslint --fix \"./src/app/**/*.ts\" -t verbose",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build -prod --aot=false --base-href /rxp-central-ihm/",
    "build-dev": "ng build -dev --environment=prod --aot=false --base-href /rxp-central-ihm/",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "typedoc": "typedoc",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.json --disableGraph",
    "prettier": "prettier --print-width 120 --single-quote --write \"./src/app/**/*.{ts,css,html}\"",
    "lint-prettify": "npm run tslint-autofix || npm run prettier"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "angular-in-memory-web-api": "^0.5.4",
    "angular2-grid": "2.1.0",
    "angular2-jwt": "0.2.3",
    "angular2-uuid": "1.1.1",
    "bootstrap": "3.3.7",
    "chart.js": "2.7.1",
    "core-js": "^2.4.1",
    "font-awesome": "4.7.0",
    "fullcalendar": "3.5.1",
    "intl": "1.2.5",
    "jquery": "3.2.1",
    "lodash": "4.17.5",
    "moment": "2.18.1",
    "ng2-translate": "5.0.0",
    "ngx-clipboard": "10.0.0",
    "prettier": "^1.11.1",
    "primeng": "5.2.3",
    "quill": "1.1.8",
    "rxjs": "^5.5.6",
    "web-animations-js": "2.3.1",
    "webpack": "3.10.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "yamljs": "0.3.0"
  }
}
